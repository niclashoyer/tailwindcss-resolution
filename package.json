{
  "name": "tailwindcss-resolution",
  "version": "0.0.0",
  "description": "A short description of the plugin",
  "main": "src/index.js",
  "scripts": {
    "release": "np",
    "test": "c8 jest"
  },
  "license": "MIT",
  "repository": "https://github.com/maizzle/tailwindcss-plugin-starter",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "c8": "^7.13.0",
    "jest": "^29.5.0",
    "np": "^8.0.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ]
  },
  "c8": {
    "exclude": [
      "jest/customMatchers.js",
      "**/*.test.js"
    ]
  },
  "keywords": [
    "tailwindcss"
  ]
}
